  "main": "index.js",
"scripts": {
    "test": "npx playwright test",
    "test:api": "npx playwright test --project=Chromium-Desktop -g 'api'", 
    "sync": "npx tsx automation/sync-manifest.ts",
    "report:allure": "npx allure generate allure-results --clean -o allure-report && npx allure open allure-report",
    "build:evidence": "npx tsx automation/consolidar-evidencia.ts",
    "report:full": "npm run sync; npm run test; npm run build:evidence",
    "docker:build": "docker build -t spei-qa-framework .",
    "docker:run": "docker run --env-file .env spei-qa-framework",
    "test:report": "npm run sync && npx playwright test && npm run build:evidence"
},
